rm.Group>\r\n              <Form.Group>\r\n                <Form.Control type=\"number\" value={tree.price ? tree.price : \"\"} name=\"price\" required onChange={handleChange} />\r\n              </Form.Group>\r\n          </Form.Row>\r\n  <Form.Group controlId=\"exampleForm.ControlSelect1\">\r\n    <Form.Label>Select pine type{<span style={{color: 'red'}}>*</span>}</Form.Label>\r\n    <Form.Control as=\"select\" value={tree.type ? tree.type : '-1'} name=\"type\" required onChange={handleChange}>\r\n      <option disabled value='-1'>--Please Choose an Option--</option>\r\n      {treeArr.map((elem, i) => {\r\n          return <option value={elem} key={i}>{elem}</option>\r\n      })}\r\n    </Form.Control>\r\n  </Form.Group>\r\n  <Form.Group controlId=\"exampleForm.ControlSelect2\">\r\n    <Form.Label>Age</Form.Label>\r\n    <Form.Control as=\"select\" name=\"age\" value={tree.age ? tree.age : '-1'} onChange={handleChange}>\r\n      <option disabled value='-1'>--Please Choose an Option--</option>\r\n      <option value={10}>10</option>\r\n      <option value={11}>11</option>\r\n      <option value={12}>12</option>\r\n      <option value={13}>13</option>\r\n      <option value={14}>14</option>\r\n      <option value={15}>15</option>\r\n    </Form.Control>\r\n  </Form.Group>\r\n  <Form.Group controlId=\"exampleForm.ControlTextarea1\">\r\n    <Form.Label>Description</Form.Label>\r\n    <Form.Control as=\"textarea\" placeholder=\"Describe Jean Pascal\" value={tree.description ? tree.description : ''} rows={3} name=\"description\" onChange={handleChange} />\r\n  </Form.Group>\r\n  <Form.Group>\r\n      {props.action === \"edit\" && tree.picture ? <img src={tree.picture} alt={tree.name} /> : '' }\r\n    <Form.File id=\"exampleFormControlFile1\" label=\"Tree Picture\" name=\"picture\" onChange={handleChange} />\r\n  </Form.Group>\r\n  <button>Submit</button>\r\n</Form></>\r\n    \r\n    )\r\n\r\n}\r\n\r\nexport default withRouter(FormTree)","C:\\Users\\Tim\\Documents\\Tim_Pe